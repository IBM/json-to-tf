{
  "name": "icse-react-assets",
  "homepage": "http://ibm.github.io/icse-react-assets",
  "version": "0.1.21",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "dependencies": {
    "@carbon/icons-react": "^11.8.0",
    "@carbon/react": "^1.13.0",
    "lazy-z": "^1.8.3",
    "react-scripts": "5.0.1",
    "regex-but-with-words": "^1.5.1",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --require @babel/register --require ignore-styles unit-tests/*.test.js",
    "coverage": "npx nyc npm run test",
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && npm run start",
    "start-storybook": "cd storybook && npm run storybook",
    "i-all": "npm i && cd playground && npm i && cd ../storybook && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground",
    "storybook": "npm-run-all --parallel build-watch start-storybook",
    "predeploy": "cd storybook && npm run build-storybook",
    "deploy-storybook": "gh-pages -d ./storybook/storybook-static"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-babel": "^6.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.3.7",
    "gh-pages": "^5.0.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2"
  }
}
