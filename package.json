{
  "name": "json-to-tf",
  "version": "0.3.0",
  "description": "A lightweight package to convert JSON into Terraform blocks. Use CDKTF json to create terraform code.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha unit-tests/ --recursive",
    "coverage": "npx nyc npm run test",
    "build": "cd playground && npm i --force && npm run build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d playground/build"
  },
  "author": "Jennifer Valle",
  "license": "Apache 2.0",
  "dependencies": {
    "lazy-z": "^1.9.0",
    "little-flute": "^0.1.0",
    "regex-but-with-words": "^1.5.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  },
  "keywords": [
    "json",
    "terraform",
    "stream",
    "cdktf",
    "iac",
    "terraform-json-parser",
    "json-to-terraform",
    "browser"
  ],
  "homepage": "http://ibm.github.io/json-to-tf",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/json-to-tf.git"
  }
}
